{
  "name": "seek-asia-style-guide",
  "version": "0.0.0-development",
  "description": "Living style guide containing the building blocks and design principles for SEEK web applications",
  "main": "index.js",
  "scripts": {
    "start": "BASE_HREF=/ node docs/server",
    "start-standalone": "IS_STANDALONE=true BASE_HREF=/ node docs/server",
    "start-in-window": "node docs/server",
    "test": "npm run test-unit && npm run lint && npm run build && npm run html-sketchapp",
    "test-unit": "BABEL_ENV=test jest --config ./jest.config.json",
    "test:ci": "BABEL_ENV=test jest --config ./jest.config.json --runInBand",
    "test-in-window": "npm run test-unit-in-window && npm run lint && npm run build",
    "test-unit-in-window": "jest --config ./jest.config.json",
    "update-snapshot": "npm run test-unit -- --updateSnapshot",
    "lint": "npm run lint-js && npm run lint-less",
    "lint-js": "eslint app standalone/*/src docs/src react test jobsDB jobStreet theme",
    "lint-less": "stylelint '{theme,react,standalone,docs,jobsDB,jobStreet}/**/*.less'",
    "clean": "rm -rf dist && rm -rf docs/dist && mkdir -p dist && mkdir -p docs/dist",
    "compile": "npm run compile-render && npm run compile-client",
    "compile-render": "NODE_ENV=production BASE_HREF=${BASE_HREF:-/} webpack --config docs/webpack.static.render.config.js && rm docs/dist/render.js",
    "compile-client": "NODE_ENV=production BASE_HREF=${BASE_HREF:-/} webpack --config docs/webpack.static.client.config.js",
    "compile-header-footer": "NODE_ENV=production IS_STANDALONE=true APP_TENANT=jobsDB webpack --config standalone/header-footer/webpack.config.js && rm dist/header-footer/render.js",
    "html-sketchapp": "html-sketchapp --config docs/html-sketchapp.config.js",
    "build": "npm run clean && npm run compile",
    "build-gh-pages": "BASE_HREF=/seek-asia-style-guide/ npm run build",
    "static": "opn http://localhost:1804 && superstatic docs/dist --port 1804",
    "deploy-gh-pages": "node scripts/deploy-gh-pages.js",
    "deploy-surge": "surge -p ./docs/dist",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "sketch-to-git": "node scripts/sketch-to-git",
    "git-to-sketch": "node scripts/git-to-sketch",
    "post-commit-status": "node scripts/post-commit-status"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seekinternational/seek-asia-style-guide.git"
  },
  "author": "SEEK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seekinternational/seek-asia-style-guide/issues"
  },
  "homepage": "https://github.com/seekinternational/seek-asia-style-guide#readme",
